{
  "name": "liqrgv/query-filter-laravel",
  "description": "Query filter shortcut for controller",
  "keywords": [
    "liqrgv",
    "laravel",
    "query filter",
    "eloquent"
  ],
  "license": "Apache-2.0",
  "type": "project",
  "require": {
    "haydenpierce/class-finder": "^0.4.0"
  },
  "require-dev": {
    "fakerphp/faker": ">=1.4",
    "illuminate/routing": ">=6.0",
    "laravel/lumen-framework": ">=6.0",
    "mockery/mockery": ">=1.0",
    "phpunit/phpunit": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "LIQRGV\\QueryFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  }
}
